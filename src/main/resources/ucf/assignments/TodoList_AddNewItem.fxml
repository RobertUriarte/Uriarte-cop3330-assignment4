<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.TodoList_Controller">
   <children>
      <TableView layoutX="-1.0" layoutY="44.0" prefHeight="355.0" prefWidth="600.0">
        <columns>
          <TableColumn onEditStart="#ChangeTitleCell" prefWidth="175.0" text="Title" />
          <TableColumn onEditStart="#ChangeDescriptionCell" prefWidth="200.0" text="Description" />
            <TableColumn onEditStart="#ChangeDueDateCell" prefWidth="100.0" text="Due Date" />
            <TableColumn onEditStart="#ChangeStatusCell" prefWidth="120.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#CancelButtonClicked" text="Cancel" />
      <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#FinishButtonClicked" text="Finish" />
   </children>
</AnchorPane>
